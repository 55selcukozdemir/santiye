<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.santiye.main.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_main_home" />

    <fragment
        android:id="@+id/navigation_equipment"
        android:name="com.example.santiye.main.EquipmentFragment"
        android:label="@string/title_equipment"
        tools:layout="@layout/fragment_main_equipment" />

    <fragment
        android:id="@+id/navigation_meterial"
        android:name="com.example.santiye.main.MeterialFragment"
        android:label="@string/title_meterial"
        tools:layout="@layout/fragment_main_meterial" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.santiye.main.Profile"
